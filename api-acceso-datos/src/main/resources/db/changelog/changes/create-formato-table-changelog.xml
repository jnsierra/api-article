<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet author="admin" id="1">
        <createTable tableName="formato">
            <column name="id" type="INT">
                <constraints primaryKey="true"/>
            </column>
            <column name="id_articulo" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="ubicacion" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="estado" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="nombre" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="formato" type="VARCHAR(255)"/>
            <column name="created_by" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="created_date" type="datetime">
                <constraints nullable="false"/>
            </column>
            <column name="last_modified_by" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="last_modified_date" type="datetime">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="formato"/>
        </rollback>
    </changeSet>
    <changeSet id="2" author="admin">
        <createSequence sequenceName="formato_seq" startValue="1" incrementBy="1" />
    </changeSet>
</databaseChangeLog>